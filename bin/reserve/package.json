{
  "name": "data-extractor",
  "version": "1.0.0",
  "description": "Salary Service Data Extractor",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "start-dev": "nodemon server.js",
    "create-migration": "npx sequelize-cli migration:generate --name",
    "migrate-up": "npx sequelize-cli db:migrate",
    "migrate-down": "npx sequelize-cli db:migrate:undo",
    "migrate-down-all": "npx sequelize-cli db:migrate:undo:all",
    "migrate-down-to": "npx sequelize-cli db:migrate:undo:all --to",
    "create-seed": "npx sequelize-cli seed:generate --name",
    "seed-up": "npx sequelize-cli db:seed:all",
    "seed-down": "npx sequelize-cli db:seed:undo",
    "seed-down-all": "npx sequelize-cli db:seed:undo:all",
    "seed-down-to": "npx sequelize-cli db:seed:undo --seed"
  },
  "author": "Malomo Damilare",
  "license": "ISC",
  "dependencies": {
    "body-parser": "~1.18.2",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "http-status-codes": "^2.1.4",
    "morgan": "^1.9.1",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.6",
    "sequelize": "^6.6.2"
  },
  "devDependencies": {
    "sequelize-cli": "^6.2.0"
  }
}
